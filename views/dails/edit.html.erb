 <main id="main" class="main">

    <div class="pagetitle">
      <h1>Data Tables</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          <li class="breadcrumb-item">Tables</li>
          <li class="breadcrumb-item active">Data</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Datatables</h5>
              
              <!-- Table with stripped rows -->
              <table class="table datatable">
                <thead>
                  
                </thead>
                <tbody>
                <%#= form_with(model: @dail, local: true) do |form| %>
                <%= form_with(model: @dail, url: update_path, method: :post) do |form| %>
                  <div class="form-group">
                    <%= form.label :product%>
                    <%= form.text_field :product, class: 'form-control' %>
                  </div>
                  <div class="form-group">
                    <%= form.label :rent %>
                    <%= form.text_area :rent, class: 'form-control' %>
                  </div>
                  <div class="form-group">
                    <%= form.label :contact %>
                    <%= form.text_area :contact, class: 'form-control' %>
                  </div>
                  <div class="form-group">
                    <%= form.submit class: 'form-control btn-primary' %>
                  </div>
                <% end %>
                  


                </tbody>
              </table>
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->
